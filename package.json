{
  "name": "ai-digest",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:azure": "bun run scripts/build-azure.js",
    "build:aws": "bun run scripts/build-aws.js",
    "build:all": "bun run build:azure && bun run build:aws",
    "build:functions": "bun run scripts/build.js",
    "zip": "bun run scripts/zip-artifacts.ts",
    "tf:init:azure": "cd terraform/azure && terraform init",
    "tf:plan:azure": "cd terraform/azure && terraform plan",
    "tf:apply:azure": "cd terraform/azure && terraform apply -auto-approve",
    "tf:init:aws": "cd terraform/aws && terraform init",
    "tf:plan:aws": "cd terraform/aws && terraform plan",
    "tf:apply:aws": "cd terraform/aws && terraform apply -auto-approve",
    "deploy:azure": "bun run build:azure && bun run zip && bun run tf:apply:azure",
    "deploy:aws": "bun run build:aws && bun run tf:apply:aws",
    "update:aws": "bun run build:aws && ./scripts/deploy-lambda.sh",
    "deploy": "bun run build:functions && bun run zip && bun run tf:apply",
    "dev:email": "email dev",
    "generate:oauth": "bun run bin/generate-oauth-token.ts",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.15",
    "@aws-sdk/client-dynamodb": "^3.868.0",
    "@aws-sdk/client-lambda": "^3.865.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-secrets-manager": "^3.864.0",
    "@aws-sdk/util-dynamodb": "^3.868.0",
    "@azure/data-tables": "^13.3.1",
    "@azure/functions": "^4.8.0-experimental.0",
    "@azure/identity": "^4.11.1",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "@react-email/tailwind": "^1.2.2",
    "ai": "^5.0.15",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "googleapis": "^144.0.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "resend": "^6.0.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@sentry/esbuild-plugin": "^4.1.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^22.17.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.9",
    "happy-dom": "^18.0.1",
    "msw": "^2.10.5",
    "react-email": "^4.2.8",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
